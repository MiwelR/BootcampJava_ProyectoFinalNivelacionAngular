{
    "extends": "tslint:recommended",
    "rulesDirectory": [
    "codelyzer"
    ],
    "rules": {
    "array-type": false,
    "arrow-parens": false,
    "deprecation": {
    "severity": "warn"
    },
    "import-blacklist": [
    true,
    "rxjs/Rx"
    ],
    "interface-name": false,
    "max-classes-per-file": false,
    "max-line-length": [
    true,
    140
   ]
}
}